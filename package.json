{
  "name": "neurolint-pro",
  "version": "1.0.0",
  "description": "NeuroLint Pro - Premium React/Next.js Code Fixing Service",
  "main": "server.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "neurolint": "node neurolint-pro.js",
    "neurolint-dry": "node neurolint-pro.js --dry-run",
    "test-engine": "curl http://localhost:3000/api/test",
    "fix-all": "node scripts/fix-layer-1-config.js && node scripts/fix-layer-2-patterns.js && node scripts/fix-layer-3-components.js",
    "fix-dry-run": "node fix-master.js --dry-run",
    "fix-verbose": "node fix-master.js --verbose",
    "fix-layer-1": "node fix-layer-1-config.js",
    "fix-layer-2": "node fix-layer-2-patterns.js",
    "fix-layer-3": "node fix-layer-3-components.js",
    "fix-layer-4": "node fix-layer-4-hydration.js",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist"
  },
  "keywords": [
    "react",
    "nextjs",
    "typescript",
    "debugging",
    "code-fixing",
    "automation",
    "ssr",
    "hydration",
    "saas",
    "premium-service"
  ],
  "author": "NeuroLint Pro",
  "license": "MIT",
  "dependencies": {
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "glob": "^8.0.3",
    "dotenv": "^16.3.1",
    "@paypal/checkout-server-sdk": "^1.0.4",
    "express-rate-limit": "^6.7.0",
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.3",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
