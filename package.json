{
  "name": "neurolint-pro",
  "version": "1.0.0",
  "description": "NeuroLint Pro - Premium React/Next.js Code Fixing Service",
  "main": "server.js",
  "bin": {
    "neurolint": "./bin/neurolint.js"
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "neurolint": "node bin/neurolint.js",
    "neurolint-legacy": "node neurolint-pro.js",
    "neurolint-enhanced": "node bin/neurolint.js --enhanced",
    "validate-ast": "node validate-ast.js",
    "test-enhanced": "node test-enhanced-ast.js",
    "collab-server": "node -e \"const { CollaborationServer } = require('./lib/collaboration-server'); const server = new CollaborationServer(8080); server.start();\"",
    "test-engine": "curl http://localhost:3000/api/test",
    "fix-all": "node scripts/fix-layer-1-config.js && node scripts/fix-layer-2-patterns.js && node scripts/fix-layer-3-components.js",
    "fix-dry-run": "node fix-master.js --dry-run",
    "fix-verbose": "node fix-master.js --verbose",
    "fix-layer-1": "node fix-layer-1-config.js",
    "fix-layer-2": "node fix-layer-2-patterns.js",
    "fix-layer-3": "node fix-layer-3-components.js",
    "fix-layer-4": "node fix-layer-4-hydration.js",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "db:setup": "psql $SUPABASE_URL -f supabase-collaboration-schema.sql"
  },
  "keywords": [
    "react",
    "nextjs",
    "typescript",
    "debugging",
    "code-fixing",
    "automation",
    "ssr",
    "hydration",
    "saas",
    "premium-service",
    "collaboration",
    "real-time"
  ],
  "author": "NeuroLint Pro",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@supabase/supabase-js": "^2.51.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.3",
    "nodemon": "^3.0.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "18.x"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
