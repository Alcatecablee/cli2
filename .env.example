# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Storage URLs - These will be automatically prefixed in Vercel
# Your Supabase database URL (same as SUPABASE_URL)
STORAGE_URL_DATABASE=https://jetwhffgmohdqkuegtjh.supabase.co
STORAGE_URL_REDIS=your_redis_connection_string
STORAGE_URL_S3=your_s3_bucket_url
STORAGE_URL_BUCKET=your_bucket_name
STORAGE_URL_CDN=your_cdn_url

# Existing environment variables (already in use)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_BASE_URL=your_base_url

# PayPal Configuration
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

# Database Configuration
DATABASE_URL=your_database_url

# API Keys
API_KEY_SECRET=your_api_key_secret

# Other Configuration
NODE_ENV=production
